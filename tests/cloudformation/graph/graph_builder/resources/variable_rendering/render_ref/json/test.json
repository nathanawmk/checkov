{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "AWS CloudFormation Template to deploy insecure infrastructure",
    "Parameters": {
        "KmsMasterKeyId": {
            "Description": "Company Name",
            "Type": "String"
        },
        "DBName": {
            "Description": "Name of the Database",
            "Type": "String",
            "Default": "db1"
        }
    },
    "Resources": {
        "MySourceQueue": {
            "Type": "AWS::SQS::Queue",
            "Properties": {
                "Tags": [
                  {
                    "Key": "yor_trace",
                    "Value": "f4a79c8c-77a5-4646-8392-6f0ff7adc960"
                  }
                ],
                "KmsMasterKeyId": {
                    "Ref": "KmsMasterKeyId"
                }
            }
        },
        "MyDB": {
            "Type": "AWS::RDS::DBInstance",
            "Properties": {
                "Tags": [
                  {
                    "Key": "yor_trace",
                    "Value": "7b93e234-b23d-432d-904e-88e5821b6ad1"
                  }
                ],
                "DBName": {
                    "Ref": "DBName"
                },
                "DBInstanceClass": "db.t3.micro",
                "Engine": "mysql",
                "MasterUsername": "master",
                "MasterUserPassword": "password"
            }
        }
    },
    "Outputs": {
        "MyDBInstanceName": {
            "Description": "My DB Instance Name",
            "Value": {
                "Ref": "MyDB"
            }
        }
    }
}
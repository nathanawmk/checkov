{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "CidrBlock": {
            "Description": "CIDR Block",
            "Type": "String",
            "Default": "172.16.0.0/16"
        }
    },
    "Resources": {
        "WebVPC": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "Tags": [
                  {
                    "Key": "yor_trace",
                    "Value": "eee9edbe-12ef-408a-8936-7e148fc8c9f5"
                  }
                ],
                "CidrBlock": {
                    "Ref": "CidrBlock"
                },
                "EnableDnsSupport": true,
                "EnableDnsHostnames": true
            }
        },
        "MySG": {
            "Type": "AWS::EC2::SecurityGroup",
            "Properties": {
                "Tags": [
                  {
                    "Key": "yor_trace",
                    "Value": "f105bc2d-44d5-42a8-b267-6cde83ed3dc9"
                  }
                ],
                "GroupName": "sg_group_name",
                "GroupDescription": "sg_group_description",
                "VpcId": {
                    "Ref": "WebVPC"
                },
                "SecurityGroupIngress": [
                    {
                        "IpProtocol": "tcp",
                        "FromPort": 3306,
                        "ToPort": 3306,
                        "CidrIp": {
                            "Fn::GetAtt": [
                                "WebVPC",
                                "CidrBlock"
                            ]
                        }
                    }
                ]
            }
        }
    }
}